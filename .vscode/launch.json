  "version": "0.2.0",
  "configurations": [
    {
      "name": "-> default",
      "request": "launch",
      "type": "dart",
      "args": [
        "--dart-define=env=dev",
        "--dart-define=app_name=fe_master_dev"
      ],
    },
    {
      "name": "-> d=chrome:5000",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "deviceId": "chrome",
      "args": [
        // "-d=chrome",
        "--web-hostname=localhost",
        "--web-port=5000",
        "--web-renderer=html"
      ],
    },
    {
      "name": "-> d=chrome:80",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "deviceId": "chrome",
      "args": [
        // "-d=chrome",
        "--web-hostname=localhost",
        "--web-port=80",
        "--web-renderer=html"
      ],
    },
    {
      "name": "-> profile mode",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "-> release mode",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    },
